FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    "(patch1|patch3)"
    {
        type            zeroGradient;
    }

    patch2
    {
        type            zeroGradient;
    }

    patch4
    {
        type            zeroGradient;
    }
}

